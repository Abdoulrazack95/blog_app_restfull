
<% include partials/header %>
<h1>Hello world</h1>

<% blogs.forEach(function(blog){ %>
    <div>
        <h1><%= blog.title %></h1>
        <img src="<%= blog.image %>">
        <span><%= blog.created %></span>
        <p><%= blog.body %></p>
        <a href="/blogs/<%= blog._id %>">Read more</a>
    </div>
<% }) %>
<% include partials/footer %>
